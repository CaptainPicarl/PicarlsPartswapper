<Window x:Class="PartSwapperGUI.XMLViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PartSwapperGUI"
        mc:Ignorable="d"
        Height="450" Width="800" Style="{StaticResource PartswapperWindowTheme}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TreeView Name="xmlTreeViewer" Grid.Column="0" Grid.Row="0" Style="{StaticResource XMLViewerTreeViewStyle}"/>
    </Grid>
</Window>
